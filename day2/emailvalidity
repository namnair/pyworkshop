#emails except yahoo and hotmail
#email ids that end with only .com,.in,.org
import re
def valid(email):
    regex = r'^(?!.*@(yahoo\.com|hotmail\.com))(?:\S+)@(?:\S+\.(?:com|in|org))$'
    if re.match(regex, email):
        return "valid"
    else:
        return "Invalid"
email= input("Enter an email address: ")
ans = valid(email)
print(ans)
